# Cloudflare Account Info
CLOUDFLARE_ACCOUNT_ID=your_account_id_here
CLOUDFLARE_API_TOKEN=your_api_token_here

# D1 Database Info
# The database ID from your Cloudflare dashboard
D1_DATABASE_ID=your_db_id_here
D1_DATABASE_NAME=your_db_name_here

# Next.js Environment Variables
NEXT_PUBLIC_ENV=development

# Neon Database URL (replace with your actual connection string)
DATABASE_URL="postgresql://<user>:<password>@<endpoint_hostname>.neon.tech/<dbname>?sslmode=require"

# Authentication
BETTER_AUTH_SECRET=

# Stripe Configuration
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=
STRIPE_BASIC_PRICE_ID=
STRIPE_PRO_PRICE_ID=
STRIPE_ENTERPRISE_PRICE_ID=